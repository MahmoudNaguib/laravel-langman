{
    "name": "mahmoudnaguib/laravel-langman",
    "description": "Manage language files with ease.",
    "keywords": ["laravel", "localization", "multilingual"],
    "homepage": "https://github.com/mahmoudnaguib/laravel-langman",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmoud Naguib",
            "email": "nagooba@gmail.com",
            "homepage": "https://mahmoudnaguib.github.io"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.8 || ^5.0",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.0",
        "mockery/mockery": "~0.9.4"
    },
    "autoload": {
        "psr-4": {
            "MahmoudNaguib\\Langman\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MahmoudNaguib\\Langman\\LangmanServiceProvider"
            ]
        }
    }
}
